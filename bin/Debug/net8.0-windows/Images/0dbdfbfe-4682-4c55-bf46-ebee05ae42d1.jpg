		<script>
    /*
     * data-parent-selector --
                             Le parent qui contient les éléments à ajouter au cart
     * data-items-selector --
                             S'il y a plusieurs éléments à l'intérieur de parent,
                             contiennent chacun la configuration JSON à envoyer
                             dans data-ga-object sinon c'est indéfini et l'item, c'est le parent
     * data-item-not-selector --
                            Si un querySelector à partir de l'item donne un résultat, on n'envoie pas d'évènement
     * data-qte-selector --
                            Quantité à l'intérieur de l'item
     * data-ga-event --
                            Sert à déterminer l'event à envoyer, si ce n'est pas défini, on prend pour acquis que c'est 'add_to_cart'

     * On s'attend à ce que la structure du DOM englobe l'ancien bouton, comme ceci:
                            <ga-add-to-cart liste des attributes>
                                <button></button> l'ancienne façon de soumettre le formulaire
                            </ga-add-to-cart>
     * et on laisse passer l'évènement après avoir reçu le clic.
     * Celui de data-parent-selector ou data-items-selector qui contient l'attribut data-ga-object
     * doit aussi avoir data-ga-event="" et le nom de l'event à envoyer à window.gaEvent().
     *
     * TODO: Ce custom-element se spécialise avec «add_to_cart» mais on pourrait le
     * rendre plus générique en utilisant le data-ga-event assorti à l'item et envoyer tous
     * les types de gaEvent
     */

    class AddToCartButton extends HTMLElement {
        constructor() {
            super();
        }

        connectedCallback() {
            this.addEventListener('click', this.click);
        }

        disconnectedCallback() {
            this.removeEventListener('click', this.click);
        }

        click(event) {
            try {
                this.configure();
                this.loopTousLesItems();
                this.sendGaEvent();
            } catch (error) {
                console.error(error);
            }
        }

        configure() {
            const parentQuiContientLesItems = this.closest(this.dataset.parentSelector);
            if (!parentQuiContientLesItems) {
                throw new Error('addToCart configuration error');
            }
            if (this.dataset.itemsSelector) {
                this.itemsQuiContiennentLeGaItem = Array.from(parentQuiContientLesItems.querySelectorAll(this.dataset.itemsSelector));
            } else {
                this.itemsQuiContiennentLeGaItem = [parentQuiContientLesItems];
            }
            this.listOfGaItems = [];
        }

        loopTousLesItems() {
            this.itemsQuiContiennentLeGaItem.forEach(item => {
                try {
                    this.ajouterUnItem(item);
                } catch (error) {
                    console.error(error);
                }
            });
        }

        ajouterUnItem(item) {
            if (this.itemEstAjoutable(item)) {
                this.addItemToCart(item);
            }
        }

        itemEstAjoutable(item) {
            if (!item) {
                return false;
            }
            if (this.dataset.itemNotSelector && item.querySelector(this.dataset.itemNotSelector)) {
                return false;
            }
            return true;
        }

        addItemToCart(item) {
            const quantiteElement = this.getQuantiteElementPourItem(item);
            this.appendGaEvent(item, quantiteElement);
        }

        getQuantiteElementPourItem(item) {
            const qteParent = this.dataset.qteParentSelector ? this.closest(this.dataset.qteParentSelector) : item;
            return qteParent.querySelector(this.dataset.qteSelector);
        }

        appendGaEvent(item, quantiteElement) {
            const gaAddToCartEvent = this.parseItemEvent(item);
            if (Array.isArray(gaAddToCartEvent)) {
                this.appendArrayToCartEvent(gaAddToCartEvent);
            } else {
                this.appendObjectToCartEvent(gaAddToCartEvent, quantiteElement);
            }
        }

        parseItemEvent(item) {
            return JSON.parse(item.dataset.gaObject ? item.dataset.gaObject : '{}');
        }

        appendArrayToCartEvent(gaAddToCartEvent) {
            this.listOfGaItems = [...this.listOfGaItems, ...gaAddToCartEvent];
        }

        appendObjectToCartEvent(gaAddToCartEvent, quantiteElement) {
            if (quantiteElement) {
                gaAddToCartEvent.quantity = quantiteElement?.value;
            }
            if (!gaAddToCartEvent.quantity) {
                return; // pas de quantité veut dire produit non disponible
            }

            this.listOfGaItems = [...this.listOfGaItems, ...[gaAddToCartEvent]];
        }

        sendGaEvent() {
