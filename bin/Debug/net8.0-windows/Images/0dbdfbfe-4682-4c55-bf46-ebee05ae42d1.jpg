			function compareAjaxHandler(data)
			{
				if(data != '')
				{
					$('#compare ul').html(data);
					initComparePopover();
				}
				//Reset checkboxes coché ou non
				if($('#compare li>.compareImg').length > 0)
				{
					$('#compareBox').show();
				}
				else
				{
					$('#compareBox').hide();
				}
				if($('#compare li>.compareImg').length > 1)
				{
					$('#compareButton').show();
				}
				else
				{
					$('#compareButton').hide();
				}

				$('input[name=compare]').prop('checked', false);
				$('input[name=compare]').change();
				$('.itemCompare.selected').removeClass('selected');
				$('.itemCompare > i').removeClass('fa-check-square-o').addClass('fa-square-o');

				$('#compare li').each(function()
				{
					id = $(this).attr('data-id');
					$('.itemCompare[data-node=' + id + ']').children("input[name='compare']").prop('checked', true);
					$('.itemCompare[data-node=' + id + ']').children("input[name='compare']").change();
					$('.itemCompare[data-node=' + id + ']').addClass('selected');
					$('.itemCompare[data-node=' + id + '] > i').removeClass('fa-square-o').addClass('fa-check-square-o');
				});
			}

			$(document).ready(function()
			{
				$('#compare ul').on('click', 'a.delete', function()
				{
					id = $(this).closest('li').attr('data-id');
					jQuery.get('https://www.richelieu.com/ca/en/compare/remove/'+id, compareAjaxHandler);
				});

				$('#compareBox').on('click', 'a.delete_all', function()
				{
					jQuery.get('https://www.richelieu.com/ca/en/compare/removeAll', compareAjaxHandler);
				});

				$('body').on('click', 'div.itemCompare label', function(e){
					e.stopPropagation();
				});

				$('body').on('click', "div.itemCompare", function(){

					if($(this).hasClass('selected')) //Remove
					{
						id = $(this).data('node');
						jQuery.get('https://www.richelieu.com/ca/en/compare/remove/'+id, compareAjaxHandler);
						$(this).children("input[name='compare']").prop('checked', false);
						$(this).removeClass('selected');
					}
					else //Add
					{
						addOk = true;
						if($('#compare li > a').length == 4)
						{
							addOk = confirm("You have already selected 4 products to compare. Would you like to delete the oldest product in order to add this one? ");
						}
						if(addOk)
						{
							id = $(this).data('node');
							jQuery.get('https://www.richelieu.com/ca/en/compare/add/'+id, compareAjaxHandler);

							$img = $(this).parent().find('img.ts-ImgMain');
							$cloneImg = $img.clone();
							offset = $img.offset();

							$cloneImg.width($img.width()).height($img.height());
							$cloneImg.css({'position': 'absolute', 'top': offset.top + 'px', 'left': offset.left + 'px', 'z-index': 3001});
							$('body').append($cloneImg);

							$('#compare').parent().show();

							$compLi = $('#compare').find('ul li.emptyLi:first');
							if(!$compLi.length)
							{
								$compLi = $('#compare').find('ul li:last');
							}
							compare_offset = $compLi.offset();

							$cloneImg.animate({
								top: compare_offset.top + 'px',
								left: compare_offset.left + 'px',
								width: '60px',
								height: '60px'
							}, 500, function() {
								$cloneImg.remove();
							});
						}
						else
						{
							$(this).children("input[name='compare']").prop('checked', false);
						}
					}
				});
			});
			
