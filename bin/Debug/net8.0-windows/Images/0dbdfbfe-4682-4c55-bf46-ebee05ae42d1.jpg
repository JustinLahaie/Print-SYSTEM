<script>
    /*
    * funnel-type --
                            Sert à déterminer le type d'événement, par défault c'est 'Click'.
    * data-ga-object --
                            Sert à définir l'objet de l'événement, si ce n'est pas défini, on tient pour acquis que c'est un objet vide.

    * On s'attend à ce que la structure du DOM englobe l'élément, comme ceci :
                            <ga-funnel-event liste des attributes>
                                <button></button> trigger de l'événement
                            </ga-funnel-event>
    * et on laisse passer l'évènement après avoir reçu le clic.
    */

    class FunnelEvent extends HTMLElement {
        constructor() {
            super();
        }

        connectedCallback() {
            this.baseGaObject = JSON.parse(this.dataset.gaObject ? this.dataset.gaObject : '{}');
            this.prefixAction = `${this.baseGaObject.event_action}`;
            if (this.dataset.funnelPlugin) {
                new funnelPlugin[`${this.dataset.funnelPlugin}`](this);
            } else {
                this[`addEventListener${this.getAttribute('funnel-type') ?? 'Click'}`]();
                if (this.hasAttribute('trigger-onload')) {
                    this.sendGaEvent();
                }
            }
        }

        disconnectedCallback() {
            this.removeEventListener('click', this.click);
            this.removeEventListener('click', this.select);
        }

        addEventListenerClick() {
            this.addEventListener('click', this.click);
        }

        addEventListenerSelect() {
            this.addEventListener('change', this.select);
        }

        addEventListenerInputRadio() {
            this.addEventListener('change', this.inputRadio);
        }

        addEventListenerSubmit() {
            this.addEventListener('submit', this.submit);
        }

        submit(event) {
            if (!this.submitFormIsWorking()) {
                return;
            }
            try {
                this.sendGaEvent();
            } catch (error) {
                console.error(error);
            }
        }

        submitFormIsWorking() {
            if (this.querySelector('div.g-recaptcha')) {
                var response = grecaptcha.getResponse();
                if (!response) {
                    return false;
                }
            }
            // on ne supporte que les forms de base. C'est sûrement incomplet pour des formulaires au fonctionnement plus complexe.
            return true;
        }

        click(event) {
            try {
                this.sendGaEvent();
            } catch (error) {
                console.error(error);
            }
        }

        select(event) {
            try {
                this.baseGaObject.event_action = `${this.prefixAction} ${event.target.value}`;
                this.sendGaEvent();
            } catch (error) {
                console.error(error);
            }
        }

        inputRadio(event) {
            try {
                if (event.target.checked) {
                    this.sendGaEvent();
                }
            } catch (error) {
                console.error(error);
            }
        }

        sendGaEvent() {
            if (window.gaActif) {
                window.dataLayer = window.dataLayer || [];
                window.dataLayer.push({ ecommerce: null });
                window.dataLayer.push(this.baseGaObject);
            }
        }
    }

    customElements.define("ga-funnel-event", FunnelEvent);
</script>
<script>
    /*
    * filter-parent-selector --
                            Sert à filtrer le sélecteur.
    * custom-type --
                            Sert à déterminer le type d'événement custom.
    * data-ga-object --
                            Sert à définir l'objet de l'événement, si ce n'est pas défini, on tient pour acquis que c'est un objet vide.

    * On s'attend à ce que la structure du DOM englobe l'élément, comme ceci :
                            <ga-custom-event liste des attributes>
                                <button></button> trigger de l'événement
                            </ga-custom-event>
    * et on laisse passer l'évènement après avoir reçu le clic.
    */

    class GaCustomEvent extends HTMLElement {
        constructor() {
            super();
        }

        connectedCallback() {
            this.gaObject = JSON.parse(this.dataset.gaObject ? this.dataset.gaObject : '{}');
            this[`addEventListener${this.getAttribute('custom-type')}`]();
        }

        disconnectedCallback() {
            this.removeEventListener('click', this.clickPopoverLogin);
            this.removeEventListener('click', this.clickFilterProducts);
        }

        addEventListenerClickPopoverLogin() {
            this.addEventListener('click', this.clickPopoverLogin);
        }

        addEventListenerClickFilterProducts() {
            this.addEventListener('click', this.clickFilterProducts);
        }

        clickPopoverLogin(event) {
            const popoverLoginElement = this.querySelector('.popover.fade.bottom.in');
            const isClickFromBtnMyProfil = event.target.closest(this.getAttribute('filter-parent-selector'));
            if (popoverLoginElement && isClickFromBtnMyProfil) {
                this.tryCatchSendGaEvent()
            }
        }

        clickFilterProducts(event) {
            const isFilterSelected = event.target.closest('.selected') !== null;
            const closestParentBoxContent = this.closest('.ts-Filters_BoxContent');
            const elementWithEventActionData = this.findSiblingWithAttributeName(closestParentBoxContent, 'data-ga-custom-event-action-selector')

            if (!isFilterSelected) {
                const eventAction = elementWithEventActionData.textContent

                try {
                    this.gaObject.event_action = eventAction
                    this.sendGaEvent();
                } catch (error) {
                    console.error(error);
                }
            }
        }

        findSiblingWithAttributeName(startElement, attributeName) {
            let previousSiblingElement = startElement.previousSibling;

            while (previousSiblingElement) {
                if (previousSiblingElement.nodeType === 1 && previousSiblingElement.hasAttribute(attributeName)) {
                    return previousSiblingElement;
                }
                previousSiblingElement = previousSiblingElement.previousSibling;
            }

            return null;
        }

        tryCatchSendGaEvent() {
            try {
                this.sendGaEvent();
            } catch (error) {
                console.error(error);
            }
        }

        sendGaEvent() {
            if (window.gaActif) {
                window.dataLayer = window.dataLayer || [];
                window.dataLayer.push({ ecommerce: null });
                window.dataLayer.push(this.gaObject);
            }
        }
    }

    customElements.define("ga-custom-event", GaCustomEvent);
</script>
